diff -Naur Compile-org/016/bin/Compile Compile/016/bin/Compile
--- Compile-org/016/bin/Compile	2024-01-04 12:00:00.000000000 +0000
+++ Compile/016/bin/Compile	2024-01-04 12:00:00.000000000 +0000
@@ -100,6 +100,15 @@
     fi
 }

+# Function to get the appropriate make command for the system
+Get_Make_Command()
+{
+    if [ "$(uname -s)" = "FreeBSD" ]; then
+        # On FreeBSD, prefer gmake (GNU make) over make (BSD make)
+        command -v gmake >/dev/null 2>&1 && echo "gmake" || echo "make"
+    else
+        echo "make"
+    fi
+}
+
 # Function to detect the build system used by the source code
 Detect_Build_System()
 {
@@ -200,7 +209,7 @@
     # Run the build
     echo "Compiling with $(Get_Make_Command)..."
-    make -j$(Get_CPU_Count) || return 1
+    $(Get_Make_Command) -j$(Get_CPU_Count) || return 1
 }

 # Install the compiled program
@@ -250,7 +259,7 @@
 Install_Program()
 {
     local install_prefix="$1"
-    make install || return 1
+    $(Get_Make_Command) install || return 1
 }

 # Clean build artifacts
@@ -270,7 +279,7 @@
 Clean_Build()
 {
     # Clean the build directory
-    make clean || return 1
+    $(Get_Make_Command) clean || return 1
 }

 # Run tests if available
@@ -290,7 +299,7 @@
 Run_Tests()
 {
     # Run tests if available
-    make check 2>/dev/null || make test 2>/dev/null || true
+    $(Get_Make_Command) check 2>/dev/null || $(Get_Make_Command) test 2>/dev/null || true
 }

 # Generate dependencies
@@ -300,7 +309,7 @@
